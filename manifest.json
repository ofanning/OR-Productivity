{
    "name": "OR Productivity",
    "version": "1.0.0",
    "description": "multifaceted productivity tool",
    "manifest_version": 3,
    "author": "Owen Fanning",
    
    "action":{
        "default_popup": "Code/layouts/index.html",
        "default_title": "OR Productivity"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["blocking.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"

    },
    "permissions": [
        "storage"
    ]
}